{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1",
  "description": "__MSG_extDescription__",
  "default_locale": "de",
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "__MSG_extName__",
    "theme_icons": [
      {
        "light": "icons/icon16.png",
        "dark": "icons/icon16-dark.png",
        "size": 16
      },
      {
        "light": "icons/icon48.png",
        "dark": "icons/icon48-dark.png",
        "size": 48
      }
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}